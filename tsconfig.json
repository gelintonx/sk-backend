{
  "compilerOptions": {
    "target": "es6",                                
    "module": "commonjs",                          
     "outDir": "./dist",                              
    "rootDir": "./src",                             
    "composite": true,                          
    "strict": true,
    "strictPropertyInitialization": false,                                 
    "esModuleInterop": true,    
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,                     
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true        
  }
}
